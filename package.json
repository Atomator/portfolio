{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "node-sass --omit-source-map-url src/assets/css/styles.scss src/assets/css/styles.css && vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-sass": "node-sass --omit-source-map-url src/assets/css/styles.scss src/assets/css/styles.css && node-sass --omit-source-map-url src/assets/css/projects.scss src/assets/css/projects.css",
    "watch-sass": "npm run build-sass -- --watch"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.0",
    "bulma": "^0.7.5",
    "bulma-timeline": "^3.0.4",
    "core-js": "^2.6.11",
    "node-sass": "^4.13.1",
    "vue": "^2.6.11",
    "vue-gtag": "^1.8.0",
    "vue-router": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^5.0.8",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-service": "^5.0.8",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
